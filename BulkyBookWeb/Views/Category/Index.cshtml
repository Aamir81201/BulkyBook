@model IEnumerable<Category>
@{
    ViewData["Title"] = "Index";
}

<h1>Index Category</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Category Name
            </th>
            <th>
                Display Orders
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var i in Model)
        {
            <tr>
                <td>
                    @i.Name
                </td>
                <td>
                    @i.DisplayOrders
                </td>
            </tr>
        }
    </tbody>
</table>
